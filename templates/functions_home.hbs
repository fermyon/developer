{{! This adds the HTML head section and the beginning of the body. See content_top.hbs. }}
{{> content_top }}

<main class="container is-fullwidth is-widescreen page-wrap">
    <div class="columns">

        {{! This adds the sidebar }}
        {{> functions_sidebar }}

        <article class="column content content-docs content-docs-wide">
            <section id="type">
                <img src="/static/image/fwf-banner.png" alt="Wasm Functions logo" style="border: none !important; box-shadow: none !important; margin: 0 0 2.5rem !important" class="no-border" />

                {{{page.body}}}

            </section>
            {{> content_footer }}
        </article>

    </div>
</main>

{{! This closes the body. See content_bottom.hbs. }}
{{> content_bottom }}

</body>

</html>