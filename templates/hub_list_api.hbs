---
read_pages_glob = ["api/hub/*.md"]
---
[
{{#each (content_filter site.pages "/content/api/hub" "hub_document")}}
{
"title": "{{this.page.head.title}}",
"category": "{{this.page.head.extra.category}}",
"language": "{{this.page.head.extra.language}}",
"author": "{{this.page.head.extra.author}}",
"summary": "{{this.page.head.extra.summary}}",
"createdAt": "{{this.page.head.extra.created_at}}",
"lastUpdated": "{{this.page.head.extra.last_updated}}",
"spinVersion": "{{{this.page.head.extra.spin_version}}}",
"url": "{{{this.page.head.extra.url}}}",
{{#if this.page.head.extra.artifact_source}}"artifactSource": "{{{this.page.head.extra.artifact_source}}}",{{/if}}
{{#with this as data}}"tags": [{{#each this.page.head.tags}} "{{this}}"{{#if @last}}{{else}},{{/if}}
{{/each}}]{{/with}},
"path": "{{{this.uri}}}"
}{{#if @last}}{{else}},{{/if}}
{{/each}}
]